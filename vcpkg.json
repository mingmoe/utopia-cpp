{
    "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg/master/scripts/vcpkg.schema.json",
    "name": "utopia",
    "description": "The game utopia",
    "version-string": "see-cmake-file",
    "dependencies": [
        "boost-stacktrace",
        "boost-asio",
        "gtest",
        "sdl2",
        "sdl2-mixer",
        "sdl2-image",
        "sdl2-gfx",
        "icu",
        {
            "name": "freetype",
            "default-features": false,
            "features": [ "brotli", "png", "bzip2", "zlib" ]
        },
        {
            "name": "harfbuzz",
            "default-features": false,
            "features": [ "icu" ]
        }
    ],
    "builtin-baseline": "07e508359ca5488456988191d8709722484daccd",
    "overrides": [
        {
            "name": "boost-backtrace",
            "version": "1.78.0"
        },
        {
            "name": "boost-asio",
            "version": "1.78.0"
        },
        {
            "name": "sdl2",
            "version": "2.0.20"
        }
    ]
}
