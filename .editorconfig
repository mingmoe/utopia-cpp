# EditorConfig is awesome: https://EditorConfig.org

# Top-most EditorConfig file
root = true

# For every file
[**]
# Unix-style newlines
end_of_line = lf

# With a newline ending
insert_final_newline = true

# UTF-8 charset
charset = utf-8

# Using 4 space instead of tab
indent_style = space
indent_size = 4
